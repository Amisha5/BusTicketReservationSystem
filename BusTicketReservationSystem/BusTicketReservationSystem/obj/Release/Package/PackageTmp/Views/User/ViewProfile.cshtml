
@{
    ViewBag.Title = "ViewProfile";
}
<style>
    body {
        /*  background: #67B26F;
        background: -webkit-linear-gradient(to right, #4ca2cd, #67B26F);
        background: linear-gradient(to right, #4ca2cd, #67B26F); */
        background: url(https://static.vecteezy.com/system/resources/previews/002/635/290/non_2x/sci-fi-futuristic-user-interface-hud-technology-abstract-background-illustration-vector.jpg);
        object-fit: cover;
        background-repeat: no-repeat;
        padding: 0;
        margin: 0;
        font-family: 'Lato', sans-serif;
        color: #000;
        background-size: cover;
        /*      backdrop-filter:blur(3px);*/
    }

    .student-profile .card {
        border-radius: 10px;
    }

        .student-profile .card .card-header .profile_img {
            width: 150px;
            height: 150px;
            object-fit: cover;
            margin: 10px auto;
            border: 10px solid #ccc;
            border-radius: 50%;
        }

        .student-profile .card h3 {
            font-size: 20px;
            font-weight: 700;
        }

        .student-profile .card p {
            font-size: 16px;
            color: #000;
        }

    #card {
        position: fixed;
        bottom: 5%;
        left: 30px;
        width: 30%;
        background-color: white;
        border-radius: 25px;
    }

    #card {
        height: 70%;
    }

    #card1 {
        left: 50%;
    }

    .card-header {
        color: #fff;
        font-weight: bold;
        margin-left: -5%;
    }
    .NameColor
    {
        color:black;
    }
</style>
<div class="student-profile py-4 bodyAlign">
    <div class="container">
        <div class="row">
            <div class="col-lg-4" id="card">
                <div class="card shadow-sm">
                    <div class="card-header bg-transparent text-center">
                        <img class="profile_img" src="data:image/png;base64," alt="dp">
                        @{
                            <h3 class="NameColor">@User.Identity.Name</h3>
                        }
                    </div>
                    <div class="card-body">
                        <p class="mb-0"><strong class="pr-1">Email:</strong></p>
                        <p class="mb-0"><strong class="pr-1">Phone Number: </strong></p>
                        @*<p class="mb-0"><strong class="pr-1">kuchh aur:</strong></p>*@
                    </div>
                </div>
            </div>
            <div class="col-lg-8" id="card1">
                <div class="card shadow-sm">
                    <div class="card-header bg-transparent border-0">
                        <div><h1>USER PROFILE</h1></div>
                    </div>

                    <div class="card col-md-3" style="width: 18rem; left:18%; top:25px;">
                        <img class="card-img-top" src="data:image/png;base64," alt="Card image cap " style="width:140px; border-radius:12%;">
                        <br />
                        <div class="card-body" style="padding-top:10px; margin-left:10px;">
                            <a href="~/User/Index" class="btn btn-primary">Booking History</a>
                        </div>
                    </div>
                    <div>
                        <br />
                        <br />
                    </div>
                    <div class="card col-md-3" style="width: 18rem; margin-top:30%; margin-left:-30%;">
                        <img class="card-img-top" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRQwPfvcBAmp1TbxCxRTLzFagVq8AQDpo8p1Q&usqp=CAU" style="width: 140px; border-radius: 12%;" />
                        <div class="card-body" style="padding-top: 10px; margin-left: 13px;">
                            <a href="~/Route/BusRoutes" class="btn btn-primary">Book New Ride</a>
                        </div>
                    </div>
                </div>
                <div>
                    <br />
                    <br />
                </div>
                <div class="card col-md-3" style="width: 18rem; margin-top: 25%; margin-left: 15%;">
                    <img class="card-img-top" src="data:image/jpeg;base64," style="width: 138px; height: 135px; border-radius: 12%;" />
                    <div class="card-body" style="padding-top: 10px; margin-left: 25px;">
                        <a href="~/Home/FeedBack" class="btn btn-primary">Feedback</a>

                    </div>
                </div>

                <div style="height: 26px"></div>
                <div class="card shadow-sm">

                </div>
            </div>
        </div>
    </div>
</div>





